<%= form_for [:admins, prize] do |f| %>
  <%= render 'shared/errors', record: prize %>

  <%= f.label :title %>
  <%= f.text_field :title, required: true %>

  <%= f.label :quantity %>
  <%= f.number_field :quantity, min: 1, step: 1, required: true %>

  <%= f.label :cost %>
  <%= f.number_field :cost, min: 1, step: 1, required: true %>

  <%= f.submit %>
<% end %>